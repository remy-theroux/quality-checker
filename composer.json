{
  "name": "remy-theroux/quality-checker",
  "description": "Gonna check quality of your PHP project",
  "keywords": [
    "cli",
    "console",
    "quality",
    "php"
  ],
  "homepage": "https://github.com/remy-theroux/quality-checker",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Rémy Théroux",
      "email": "remy.theroux@iadvize.com"
    }
  ],
  "require": {
    "symfony/console": "^2.6",
    "monolog/monolog": "^1.11",
    "symfony/yaml": "^2.7.5",
    "squizlabs/php_codesniffer": "^2.0",
    "escapestudios/symfony2-coding-standard": "^2.0",
    "symfony/process": "^2.7",
    "doctrine/collections": "^1.3",
    "symfony/dependency-injection": "^2.7",
    "symfony/config": "^2.7",
    "symfony/expression-language": "^2.7",
    "phpmd/phpmd": "^2.3",
    "symfony/event-dispatcher": "^2.7",
    "phpunit/phpunit": "^5.0",
    "mockery/mockery": "^0.9.4"
  },
  "require-dev": {
    "iadvize/php-convention": "dev-master"
  },
  "bin": [
    "bin/qualitychecker"
  ],
  "autoload": {
    "psr-4": {
      "QualityChecker\\": [
        "src"
      ]
    }
  }
}
