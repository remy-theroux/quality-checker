{
  "name": "remy-theroux/quality-checker",
  "description": "Gonna check quality of your PHP project",
  "keywords": [
    "cli",
    "console",
    "command",
    "quality",
    "php"
  ],
  "version": "0.1.0",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Rémy Théroux",
      "email": "remy.theroux@iadvize.com"
    }
  ],
  "require": {
    "symfony/console": "2.6.*@dev",
    "symfony/yaml": "2.6.x-dev",
    "monolog/monolog": "~1.11",
    "symfony/yaml": "^2.7.5",
    "squizlabs/php_codesniffer": "2.*",
    "escapestudios/symfony2-coding-standard": "~2.0",
    "symfony/process": "^2.7",
    "doctrine/collections": "^1.3",
    "symfony/dependency-injection": "^2.7",
    "symfony/config": "^2.7",
    "symfony/expression-language": "^2.7"
  },
  "bin": [
    "bin/quality-checker"
  ],
  "scripts": {
    "post-install-cmd": [
      "./vendor/bin/phpcs --config-set installed_paths $PWD/vendor/escapestudios/symfony2-coding-standard"
    ]
  },
  "autoload": {
    "psr-4": {
      "QualityChecker\\": [
        "src"
      ]
    }
  }
}
